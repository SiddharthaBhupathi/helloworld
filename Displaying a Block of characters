namespace program
{
    class square
    {
        static void Main()
        {
            {
                int i = 10;
                int j = 10;
                for (i = 1; i <=10; i++)
                {
                    for (j = 1; j <= 10; j++)
                    {
                        Console.WriteLine("*");
                    }
                    Console.WriteLine("\n");
                }
            }
        }
    }
}
