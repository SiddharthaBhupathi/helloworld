
namespace programs
{
    class square
    {
        static void Main(String[]args)
        {
            {
                int i, j;
                int N = 10;
                for (i = 1; i <= N; i++)
                {
                    for (j = 1; j <= N; j++)
                    {
                        Console.Write("*");
                    }
                    Console.Write("\n");
                }

            }
        }
    }
}
